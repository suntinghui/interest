id\">\r\n                        <template slot=\"title\">\r\n                            <Icon :type=\"pmenu.icon\"></Icon>\r\n                            {{pmenu.name}}\r\n                        </template>\r\n                        <MenuItem v-for=\"cmenu in pmenu.children\" :name=\"cmenu.id\" :key=\"cmenu.id\">\r\n                            <Icon :type=\"cmenu.icon\"></Icon>\r\n                            {{cmenu.name}}\r\n                        </MenuItem>\r\n                    </Submenu>\r\n                </Menu>\r\n            </Col>\r\n            <Col span=\"20\">\r\n                <div class=\"layout-header\">\r\n                    <Menu mode=\"horizontal\" theme=\"light\"  :style=\"{height:'65px',width:'100%'}\"  @on-select=\"m=>{menuSelect(m)}\" >\r\n                        <div style=\"width: 95%;margin: 0 auto\">\r\n                            <div class=\"layout-nav\">\r\n                                <MenuItem name=\"2\">\r\n                                    <Icon type=\"ios-person\" size=\"22\"></Icon>\r\n                                    {{userName}}\r\n                                </MenuItem>\r\n                                <MenuItem name=\"1\">\r\n                                    <Icon type=\"log-out\"></Icon>\r\n                                    退出\r\n                                </MenuItem>\r\n                            </div>\r\n                        </div>\r\n                    </Menu>\r\n                </div>\r\n                <div class=\"layout-breadcrumb\">\r\n                    <Breadcrumb>\r\n                        <BreadcrumbItem v-for=\"item in breadcrumbData\" :to=\"item.url\" :key=\"item.id\">{{item.name}}</BreadcrumbItem>\r\n                    </Breadcrumb>\r\n                </div>\r\n                <div class=\"layout-content\">\r\n                    <router-view></router-view>\r\n                </div>\r\n                <div class=\"layout-copy\">\r\n                    2018-2020 &copy; 北京万联国通通信技术有限公司\r\n                </div>\r\n            </Col>\r\n        </Row>\r\n    </div>\r\n</template>\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      /*用户名*/\r\n      userName: null,\r\n      /*一级菜单*/\r\n      menuList: [],\r\n      /*二级菜单*/\r\n      menuSub: [],\r\n      /*面包屑data*/\r\n      breadcrumbData: []\r\n    };\r\n  },\r\n  mounted() {\r\n    /*this.$router.push();*/\r\n    /*this.userName = window.localStorage.getItem(\"currentUser_name\");*/\r\n    this.axios({\r\n      method: \"get\",\r\n      url: \"/public/user\"\r\n    })\r\n      .then(\r\n        function(response) {\r\n          this.userName = response.data.data.name;\r\n        }.bind(this)\r\n      )\r\n      .catch(function(error) {\r\n        alert(error);\r\n      });\r\n    this.axios({\r\n      method: \"get\",\r\n      url: \"/manage/menu\"\r\n    })\r\n      .then(\r\n        function(response) {\r\n          this.menuList = response.data.data;\r\n          for (var i in this.menuList) {\r\n            for (var j in this.menuList[i].children) {\r\n              this.menuSub.push(this.menuList[i].children[j]);\r\n            }\r\n          }\r\n        }.bind(this)\r\n      )\r\n      .catch(function(error) {\r\n        alert(error);\r\n      });\r\n  },\r\n  methods: {\r\n    menuSelect(e) {\r\n      if (e == 1) {\r\n        this.$store.dispatch(\"users/loginOUt\", { router: this.$router });\r\n      }\r\n    },\r\n    /*菜单选择事件*/\r\n    select(e) {\r\n      var filterMenus = this.menuSub.filter(function(menu) {\r\n        return menu.url != null && menu.url != \"\" && menu.id == e;\r\n      });\r\n      this.$router.push(filterMenus[0].url);\r\n      this.breadcrumbData.splice(0, 1, filterMenus[0]);\r\n    },\r\n    /*下拉菜单选择事件*/\r\n    dropdownSelect(e) {\r\n      this.$store.dispatch(\"users/loginOUt\", { router: this.$router });\r\n    },\r\n    backHome() {\r\n      this.$router.push(\"/base\");\r\n    }\r\n  }\r\n};\r\n</script>\r\n"],"sourceRoot":"src/v